# Programa contador
# El registro x5 se incrementa indefinidamente
# El registro x6 se incrementa de dos en dos

	.text

inicio:
	# x5 = 0
	addi x5, x0, 0
etiqueta1:	
	# x6 = 0
	addi x6, x0, 0
bucle: 
	# x5 = x5 + 1
	addi x5, x5, 1
etiqueta2:	# x6 = x6 +2
	addi x6, x6, 2
	#Repetir indefinidamente
	b bucle
fin: 